//---обертка двух первых секций для параллакса

.wrapper-section {
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
  position: relative;
}
//---паралкс блок
.parallax {
  position: absolute;
  width: 100%;
  height: 100%;
  display: flex;
  @include phones {
  }
}
//----картинка первой секеции велком
.parallax-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}